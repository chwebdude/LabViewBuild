{
    "manifestVersion": 1,
    "id": "labview-build",
    "name": "LabView Build Tools",
    "version": "1.0.0",
    "publisher": "chwebdude",
    "public": true,
    "tags": [
        "Build",
        "NI",
        "LabVIEW"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "NI LabVIEW Build tools",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon128.png",
        "large": "images/icon512.png"
    },
    "files": [
        {
            "path": "buildtask"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildtask"
            }
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "screenshots": [
        {
            "path": "images/Screenshot1.png"
        },
        {
            "path": "images/Screenshot2.png"
        },
        {
            "path": "images/Screenshot3.png"
        }
    ],
    "links": {
        "repository": {
            "uri": "https://github.com/chwebdude/LabViewBuild"
        },
        "issues": {
            "uri": "https://github.com/chwebdude/LabViewBuild/issues"
        },
        "support": {
            "uri": "https://github.com/chwebdude/LabViewBuild"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/chwebdude/LabViewBuild"
    },
    "badges": [
        {
            "href": "https://dev.azure.com/webdude/DevOps%20Extensions/_build?definitionId=26&_a=summary",
            "uri":"https://dev.azure.com/webdude/DevOps%20Extensions/_apis/build/status/LabViewBuild",
            "description": "CI Build" 
        }
    ]
}